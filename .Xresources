! settings for yaahconsole

!yeahconsole*consoleHeight: 24
!yeahconsole*toggleKey: None+Menu
!yeahconsole*foreground: green
!yeahconsole*background: black
!yeahconsole*aniDelay: 10
!yeahconsole*faceName: Monospace:style=Regular
!yeahconsole*faceSize: 11


yeahconsole*termName: rxvt-256color
yeahconsole*consoleHeight: 30
yeahconsole*toggleKey: None+Menu
yeahconsole*background: #212121
yeahconsole*foreground: #C0C0C0
yeahconsole*cursorColor: #E6E3E6
yeahconsole*restart: 1
yeahconsole*keyFull: None+F11
yeahconsole*keySmaller: Control+KP_Subtract
yeahconsole*keyBigger: Control+KP_Add
!yeahconsole*faceName: xft:Consolas:size=12
!yeahconsole*boldFont: xft:Consolas:bold:size=12
yeahconsole*metaSendsEscape: true
yeahconsole*utf8: 2
yeahconsole*transparent: true
yeahconsole*shading: 4
yeahconsole*depth: 32
yeahconsole*letterSpace: -1
