! ----------------------------------------------------------------------------
! file:     ~/.Xresources
! vim: ft=xdefaults
! ----------------------------------------------------------------------------
 
xterm*metaSendsEscape: True
xterm*saveLines: 1024
xterm*background: black
xterm*foreground: #DDDDDD
xterm*cursorColor: green
XTerm*cursorBlink: false
xterm*cursorOnTime:	550
xterm*cursorOffTime:	150
! xterm*faceName: PragmataPro:pixelsize=15
xterm*faceName:         Terminus:pixelsize=14
xterm*Geometry: 120x60+100+60

xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      true
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          true
xterm*rightScrollBar:     true
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*toolBar:            true

xterm*color0:    #000000
xterm*color1:    #CC0000
xterm*color2:    #4E9A06
xterm*color3:    #C4A000
xterm*color4:    #3465A4
xterm*color5:    #75507B
xterm*color6:    #06989A
xterm*color7:    #D3D7CF
xterm*color8:    #555753
xterm*color9:    #EF2929
xterm*color10:   #8AE234
xterm*color11:   #FCE94F
xterm*color12:   #729FCF
xterm*color13:   #AD7FA8
xterm*color14:   #34E2E2
xterm*color15:   #EEEEEC
xterm*termName: xterm-256color

                            
!Xft.autohint: 0
!Xft.lcdfilter:  lcddefault
!Xft.hintstyle:  hintfull
!Xft.hinting: 1
!Xft.antialias: 1
!Xft.rgba: rgb
!Xft.dpi: 90
 
urxvt*geometry: 150x50+100+60
! latest
!urxvt*font:            xft:consolas:pixelsize=15:autohint=true
!urxvt*boldFont:        xft:consolas:bold:pixelsize=15:autohint=true
!urxvt*italicFont:      xft:consolas:italic:pixelsize=15:autohint=true
!urxvt*bolditalicFont:  xft:consolas:bolditalic:pixelsize=15:autohint=true
urxvt*letterSpace: 0
urxvt*lineSpace: 0

!urxvt*font:            xft:inclonsolata:size=10
!urxvt*boldFont:        xft:inclonsolata:bold:size=10
!urxvt*italicFont:      xft:consolas:italic:size=10
!urxvt*bolditalicFont:  xft:consolas:bolditalic:size=10
